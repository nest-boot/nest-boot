  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start --watch --entryFile repl",
